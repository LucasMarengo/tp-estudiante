@model tpEstudiante.Materia

@{
    ViewData["Title"] = "Detalle";
}

<h1>Detalle de la Materia</h1>

<div>

    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nombre)
