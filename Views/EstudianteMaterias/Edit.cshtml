@model tpEstudiante.EstudianteMateria

@{
    ViewData["Title"] = "Editar";
}

<h1>Editar Relacion</h1>

<hr />
<div class="row">
    <div class="col-md-8">
        <form asp-action="Edit" asp-controller="EstudianteMaterias">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="EstudianteId" />
            <input type="hidden" asp-for="MateriaId" />

            <div class="form-group">
                <label asp-for="EstudianteId" class="control-label"></label>
                <select asp-for="EstudianteId" class="form-control" asp-items="ViewBag.EstudianteId" disabled></select>
            </div>
            <div class="form-group">
                <label asp-for="MateriaId" class="control-label"></label>
                <select asp-for="MateriaId" class="form-control" asp-items="ViewBag.MateriaId" disabled></select>
            </div>

            <div class="form-group">
                <label asp-for="FechaInscripcion" class="control-label"></label>
                <input asp-for="FechaInscripcion" class="form-control" />
                <span asp-validation-for="FechaInscripcion" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Modificar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index" class="btn btn-info">Volver</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
